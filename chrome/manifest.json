{
  "name": "Duizend Bookmarks",
  "description": "Display the bookmarks you want on the new tab page",
  "version": "0.1",
  "permissions": [
    "bookmarks"
  ],
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
